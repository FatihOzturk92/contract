{
  "display": "form",
  "components": [
    {
      "title": "Document Group",
      "collapsible": false,
      "key": "documentGroup",
      "type": "panel",
      "label": "Panel",
      "input": false,
      "tableView": false,
      "components": [
        {
          "label": "Code",
          "applyMaskOn": "change",
          "tableView": true,
          "key": "code",
          "type": "textfield",
          "input": true
        },
        {
          "label": "Titles",
          "reorder": false,
          "addAnotherPosition": "bottom",
          "layoutFixed": false,
          "enableRowGroups": false,
          "initEmpty": false,
          "tableView": false,
          "defaultValue": [
            {}
          ],
          "key": "titles",
          "type": "datagrid",
          "input": true,
          "components": [
            {
              "label": "Language",
              "widget": "choicesjs",
              "tableView": true,
              "dataSrc": "url",
              "data": {
                "url": "https://test-amorphie-contract.burgan.com.tr/Common-LanguageType?page=0&pageSize=10",
                "headers": [
                  {
                    "key": "",
                    "value": ""
                  }
                ]
              },
              "valueProperty": "id",
              "template": "<span>{{ item.code }}</span>",
              "key": "language",
              "type": "select",
              "disableLimit": false,
              "noRefreshOnScroll": false,
              "input": true
            },
            {
              "label": "Title",
              "applyMaskOn": "change",
              "tableView": true,
              "key": "title",
              "type": "textfield",
              "input": true
            }
          ]
        },
        {
          "label": "Documents",
          "reorder": false,
          "addAnotherPosition": "bottom",
          "layoutFixed": false,
          "enableRowGroups": false,
          "initEmpty": false,
          "tableView": false,
          "defaultValue": [
            {}
          ],
          "key": "documents",
          "type": "datagrid",
          "input": true,
          "components": [
            {
              "label": "Document",
              "widget": "choicesjs",
              "tableView": true,
              "dataSrc": "url",
              "data": {
                "url": "https://test-amorphie-contract.burgan.com.tr/document-definition/GetAllSearch?Keyword=%20",
                "headers": [
                  {
                    "key": "",
                    "value": ""
                  }
                ]
              },
              "valueProperty": "id",
              "template": "<span>{{ item.title.name }}</span>",
              "key": "document",
              "type": "select",
              "disableLimit": false,
              "noRefreshOnScroll": false,
              "input": true
            }
          ]
        }
      ]
    }
  ]
}